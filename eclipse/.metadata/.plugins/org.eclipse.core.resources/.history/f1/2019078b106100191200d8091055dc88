	public SaveBinds(String name, String decription) {
		super(name, decription);
		filePath = Whirl.instance.directory.getAbsolutePath();
	}
	
	public String filePath;
	
	@Override
	public void execute(String[] args) {
		if (args.length != 0) {
			msgWithPrefix("¡ìcParameter error: ¡ì7$save");
			return;
		}
		System.out.println(Whirl.instance.directory.getAbsolutePath());
		
	}

}
